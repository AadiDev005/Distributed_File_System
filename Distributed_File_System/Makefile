check-deps: ## Verify toolchain & create directories
	@echo "ðŸ” Checking DataVault Enterprise dependencies..."
	@command -v go >/dev/null 2>&1 || { echo "âŒ Go not installed"; exit 1; }
	@command -v node >/dev/null 2>&1 || { echo "âŒ Node.js not installed"; exit 1; }
	@command -v npm >/dev/null 2>&1 || { echo "âŒ npm not installed"; exit 1; }
	@mkdir -p ./storage/shared ./logs ./tmp
	@echo "âœ… Enterprise dependencies verified, directories created"
	@$(MAKE) install-cors

