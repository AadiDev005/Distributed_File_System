workflow-test: ## Test complete workflow management system
	@echo "ðŸ“‹ Testing DataVault Workflow Management System..."
	@echo "ðŸ” Testing workflow status..."
	@curl -s http://localhost:8080/api/workflow/status | jq '.' || echo "âŒ Workflow status failed"
	@echo "ðŸ“ Testing workflow templates..."
	@curl -s http://localhost:8080/api/workflow/templates | jq '.templates | length' | xargs echo "Templates found:"
	@echo "ðŸš€ Testing workflow start..."
	@curl -s -X POST -H "Content-Type: application/json" \
		-d '{"workflow_id":"template_001","user_id":"test","variables":{"test":true}}' \
		http://localhost:8080/api/workflow/start | jq '.success' || echo "âŒ Workflow start failed"

